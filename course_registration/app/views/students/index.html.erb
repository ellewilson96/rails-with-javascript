  <h3>All Flatiron High School Students</h3>
  <% @students.each do |student| %>
  <h3><%= student.first_name %> <%= student.last_name%></h3>
  <%= link_to "View #{student.first_name}'s Grades", student_grades_path(student) %><br>
  <%= link_to "Add New Grade for #{student.first_name}", new_student_grade_path(student) %>
<% end %>
<h3><%= link_to "Enroll a New Student", new_student_path %>
<%= link_to "Top 3 Students", students_smartest_path %></h3>
